<layout version="0.1.0">
	<adminhtml_pagebuilder_pagebuilder_index>
		<reference name="content">
			<block type="pagebuilder/adminhtml_pagebuilder" name="pagebuilder" />
		</reference>
	</adminhtml_pagebuilder_pagebuilder_index>

	<adminhtml_pagebuilder_pagebuilder_grid>
		<reference name="content">
			<block type="pagebuilder/adminhtml_pagebuilder_grid" name="adminhtml.pagebuilder.grid" />
		</reference>
	</adminhtml_pagebuilder_pagebuilder_grid>

	<adminhtml_pagebuilder_pagebuilder_loadindex>
		<block type="pagebuilder/adminhtml_pagebuilder_addwidget" name="adminhtml.pagebuilder_addwidget" output="toHtml" />
	</adminhtml_pagebuilder_pagebuilder_loadindex>

	<adminhtml_pagebuilder_pagebuilder_loadoptions>
		<block name="pagebuilder.wysiwyg_widget.options" type="pagebuilder/adminhtml_pagebuilder_addwidget_options" output="toHtml" />
	</adminhtml_pagebuilder_pagebuilder_loadoptions>
</layout>